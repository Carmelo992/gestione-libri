extension MODULE_PASCALDaoExt on MODULE_PASCALDao {
  List<Row> getMODULE_PASCALs({PaginationDaoModel? pagination}) =>
      CrudDao.getAll(db, MODULE_PASCALDao.tableName, pagination: pagination, orderByField: [MODULE_PASCALDaoModel.nameKey]);

  Row insertMODULE_PASCAL(MODULE_PASCALDaoModel MODULE_CAMEL) => CrudDao.insert(db, MODULE_PASCALDao.tableName, [
    MODULE_PASCALDaoModel.nameKey,
    //COLUMN_NAME_PLACE_HOLDER
    //Do not remove the line above

  ], [
    MODULE_CAMEL.name,
    //COLUMN_VALUE_PLACE_HOLDER
    //Do not remove the line above
  ]);

  Map<String, dynamic>? getMODULE_PASCAL(String MODULE_CAMELId) => CrudDao.getById(db, MODULE_PASCALDao.tableName, MODULE_CAMELId);

  Row? updateMODULE_PASCAL(UpdateMODULE_PASCALDaoModel MODULE_CAMEL, String MODULE_CAMELId) => CrudDao.update(
    db,
    MODULE_PASCALDao.tableName,
    MODULE_CAMELId,
    [
        if (MODULE_CAMEL.name != null) UpdateMODULE_PASCALDaoModel.nameKey,
        //COLUMN_UPDATE_PLACE_HOLDER
        //Do not remove the line above
    ],
    [
        if (MODULE_CAMEL.name != null) MODULE_CAMEL.name,
        //COLUMN_UPDATE_VALUE_PLACE_HOLDER
        //Do not remove the line above
    ],
  );

  bool deleteMODULE_PASCAL(String MODULE_CAMEL) => CrudDao.delete(db, MODULE_PASCALDao.tableName, MODULE_CAMEL);
}
